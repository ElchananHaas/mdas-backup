function toggleLayer(label) {

    // get the element
  	var layer = document.getElementById(label);
    var layerStyle = layer.style||layer;

    // toggle
    if(layerStyle.visibility != 'hidden') {
      layerStyle.visibility = 'hidden';
      layerStyle.display = 'none';
    } else {
      layerStyle.visibility = 'visible';
      layerStyle.display = 'block';
    }
}

